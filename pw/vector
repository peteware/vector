#ifndef PW_VECTOR_H   // -*- c++ -*-
#define PW_VECTOR_H

#include <pw/allocator>
#include <pw/memory_resource>

namespace pw {

template<class Type, class Allocator = pw::allocator<Type> >
class vector
{
public:
};

namespace pmr {
template <class Type>
using vector = pw::vector<Type, pw::pmr::polymorphic_allocator<Type> >;
}

}

#endif /*  PW_VECTOR_H */
